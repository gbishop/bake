# Simple test loaf

starter ^ 5%:
  ww_flour
  water
  hydration = 100%

dough:
  starter = 10g
  prairie_gold = 300% * _part
  bronze_chief = 2 * _part
  spelt = _part / 2
  water
  yeast = 0.4%
  hydration = 70%
  _muffins = 10
  _size = 46g
  total_flour = 9 / 9 * _muffins * _size
  salt = 2%
