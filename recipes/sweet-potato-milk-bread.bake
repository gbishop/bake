# Sweet potato milk bread

# From: https://www.thefreshloaf.com/node/75329/85-hydration-no-water-added-sd-sweet-potato-milk-loaf

starter:
  bran = 8 * _part
  sugar = 1 * _part
  water = 3 * _part
  total = 0.5%

leven:
  starter
  sweet_potato
  ap_flour = 20% * sweet_potato
  water
  skim_milk_powder = 8.8%
  brown_sugar = 10%
  total_flour = 20%

dough:
  leven
  bread_flour
  vwg = 3.5%
  salt = 1.5%
  egg = 8.8%
  yeast = 0.5%

  total_flour = 570g
  hydration = 85%
  _serving = 0



