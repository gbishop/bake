# Simple test loaf

starter ^ 5%:
  ww_flour
  water
  hydration = 100%

dough:
  starter = 10g
  /* names that begin with _ are not ingredients but are
     used for calculations */
  prairie_gold = 300% * _part
  bronze_chief = 2 * _part
  spelt = _part / 1
  water
  salt = 2% * (prairie_gold + bronze_chief + spelt)
  yeast = 0.4%
  hydration = 70%
  total_flour = 460g

