# My everyday loaf based on morph

leaven ^ 5g:
  rye
  flas
  hydration = 100%

scald ^ 5g:
  potato_flakes = 3%
  flaxseed_meal = 5%
  boiling_water = 3 / 4 * total

grain:
  prairie_gold
  hard_red
  spelt = 5%
  barley = 5%

dry:
  grain
  vwg = 3%
  salt = 1.8%
  yeast = 0.5%
  diastatic_malt = 0.25%
  ascorbic_acid = 0.08%

wet:
  leaven = 20%
  scald
  flas
  water
  flas = water
  olive_oil = 5%
  honey = 5%
  buttermilk = 5%

dough:
  wet
  dry
  hydration = 70%
  # 200g/L of flour
  total_flour = 500g # 220g

