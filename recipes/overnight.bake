# Trying overnight cold proof without preferment

starter:
  whole_wheat = 2%
  water
  hydration = 75%

grain:
  prairie_gold
  hard_red = 30%
  spelt = 10%
  rye = 5%
  oats = 5%

dry:
  grain
  # half the water to milk
  nido = 50% * 13% * dough.total_water
  flaxseed_meal = 3%
  potato_flakes = 2%
  salt = 2%
  wgbi = 3%
  yeast = 0.5%

wet:
  starter
  water
  olive_oil = 5%
  honey = 5%
  yogurt = 5%

dough:
  wet
  dry
  hydration = 75%
  total_flour = 200g

