# Start with soft white bread and morph to whole grain

/*
This is a block comment.
*/

starter:
  rye
  water
  hydration = 60%

sponge + 9.6g:
  starter = 0.2 * total_flour
  water
  oats = ww_flour
  ww_flour
  total_flour = 15%
  hydration = 100%

grain:
  prairie_gold
  hard_red
  spelt = 10%
  rye = 5%

wet:
  sponge
  water
  butter
  honey = 8%

dry:
  grain
  nido = 0.50 * 0.13 * wet.water
  potato_flakes = 2%
  flaxseed_meal = 7%
  salt = 1.8% - 0.013 * wet.butter
  wgbi = 2%
  yeast = 0.4%

dough:
  wet
  dry
  total_water = 70%
  total_fat = 8%
  total_flour = 480g

